  },
  "bugs": "https://github.com/Mause/vercel-openapi/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/pino": "^6.3.11",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.1.0",
    "openapi3-ts": "^2.0.1",
    "pino": "^6.13.2",
    "pino-pretty": "^7.0.0",
    "ts-node": "^10",
    "tslib": "^2",
    "typescript": "^4.4",
    "yaml": "^2.0.0-8"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^9",
    "@types/node": "^16",
    "@vercel/node": "*",
    "chai": "^4",
    "eslint": "^7.32",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "mocha": "^9",
    "mocha-chai-jest-snapshot": "^1.1.3",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/Mause/vercel-openapi",
  "keywords": [
    "oclif",
    "vercel",
    "openapi"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "vercel-openapi",
    "repositoryPrefix": "https://github.com/Mause/vercel-openapi",
    "commands": "src/commands"
  },
  "repository": "Mause/vercel-openapi",
  "scripts": {
    "build": "tsc --noemit",
    "posttest": "echo 0 || eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "peerDependencies": {
    "@types/node": "^16",
    "@vercel/node": "*"
  }
