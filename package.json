{
  "name": "vercel-openapi",
  "version": "0.1.16",
  "author": "Elliana May @Mause",
  "bin": {
    "vercel-openapi": "./bin/run"
  },
  "bugs": "https://github.com/Mause/vercel-openapi/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5.2.7",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "@types/glob": "^8.0.0",
    "@types/validator": "^13.7.10",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.1",
    "glob": "^8.0.0",
    "isomorphic-git": "^1.21.0",
    "openapi3-ts": "^3.0.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "ts-node": "^10",
    "tslib": "^2",
    "typescript": "^4.8",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.10",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18",
    "@vercel/node": "*",
    "chai": "^4",
    "eslint": "^8.27.0",
    "eslint-config-oclif": "^4.0",
    "eslint-config-oclif-typescript": "^1.0",
    "husky": ">=8",
    "lint-staged": ">=13",
    "mocha": "^10.1.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "oclif": "^3.2.25",
    "prettier": "^2.7.1",
    "shx": "^0.3.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/Mause/vercel-openapi",
  "keywords": [
    "oclif",
    "vercel",
    "openapi"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "vercel-openapi",
    "repositoryPrefix": "https://github.com/Mause/vercel-openapi",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "commands": "./dist/commands"
  },
  "repository": "Mause/vercel-openapi",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "prepack": "yarn build && oclif manifest && node fail_on_empty.mjs && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "yarn build && oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@types/node": "^18",
    "@vercel/node": "*"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write"
  }
}
